func init(truth)
{
    if(truth) 
    {
        print(not_defined_yet);
    }
    else
    {
        not_defined_yet = 1;
    }
}

/* Make sure to clear the symbol table on the way out of the funciton */
init(0);
/* Because this call will actually print 1, even though the name isn't 
   defined yet
*/
init(1);
